<odoo>

    <record model="ir.ui.view" id="res_config_settings_view_form">
        <field name="name">res.config.settings.form (in google_calendar_name)</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="google_calendar.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <field name="cal_client_secret" position="after">
                <label for="cal_calendar_name" string="Calendar name" class="col-xs-3 col-md-3 o_light_label"/>
                <field name="cal_calendar_name" nolabel="1"/>
            </field>
        </field>
    </record>



</odoo>
